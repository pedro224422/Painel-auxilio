<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    
    <link rel="manifest" href="manifest.json">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <link rel="icon" href="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRh3f1D8_Opxr49pY6wH4vA-hL5-Uj8uYfHpg&usqp=CAU">
    
    <title>Auxílio @Pg Premium</title>
    <link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700&display=swap" rel="stylesheet">
    <style>
        :root {
            --main-red: #ff0000;
            --dark-red: #3d0a0a;
            --bg-black: #0a0a0b;
        }

        body {
            background-color: #000;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
            margin: 0;
            overflow: hidden;
            font-family: 'Orbitron', sans-serif;
        }

        #particles { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: -1; }

        #fov-circle {
            position: absolute; top: 50%; left: 50%;
            transform: translate(-50%, -50%);
            border: 2px solid var(--main-red);
            border-radius: 50%;
            pointer-events: none;
            display: none;
            z-index: 999;
            box-shadow: 0 0 15px rgba(255, 0, 0, 0.5);
        }

        .container {
            background-color: rgba(10, 10, 12, 0.98);
            width: 440px;
            padding: 20px;
            border-radius: 12px;
            border: 2px solid var(--dark-red);
            box-shadow: 0 0 30px rgba(0, 0, 0, 1);
            color: white;
            position: relative;
            backdrop-filter: blur(10px);
            z-index: 10;
            transition: all 0.4s ease;
            animation: borderPulse 3s infinite alternate;
        }

        @keyframes borderPulse {
            from { border-color: var(--dark-red); box-shadow: 0 0 10px rgba(255, 0, 0, 0.2); }
            to { border-color: var(--main-red); box-shadow: 0 0 25px rgba(255, 0, 0, 0.5); }
        }

        h2 {
            text-align: center;
            color: var(--main-red);
            text-transform: uppercase;
            letter-spacing: 4px;
            margin-bottom: 20px;
            font-size: 18px;
            text-shadow: 0 0 10px rgba(255, 0, 0, 0.7);
        }

        .main-layout { display: flex; gap: 15px; }
        .sidebar { display: flex; flex-direction: column; gap: 8px; width: 90px; }

        .nav-item {
            background: #111;
            border: 1px solid #222;
            border-radius: 6px;
            padding: 12px 5px;
            text-align: center;
            font-size: 9px;
            cursor: pointer;
            transition: 0.2s;
            color: #666;
            text-transform: uppercase;
        }

        .nav-item.active {
            background: var(--main-red);
            color: white;
            box-shadow: 0 0 15px rgba(255, 0, 0, 0.4);
            border-color: #ff4d4d;
        }

        .content-area { flex-grow: 1; min-height: 220px; }
        .tab-content { display: none; }
        .tab-content.active { display: block; }
        .options-grid { display: grid; grid-template-columns: 1fr 1fr; gap: 10px; }

        .option-box {
            background: rgba(20, 20, 25, 0.8);
            padding: 12px;
            border-radius: 6px;
            display: flex;
            justify-content: space-between;
            align-items: center;
            font-size: 9px;
            border: 1px solid #222;
        }

        .switch-container { position: relative; width: 34px; height: 18px; }
        .switch-container input { opacity: 0; width: 0; height: 0; }
        .slider-switch {
            position: absolute; cursor: pointer; top: 0; left: 0; right: 0; bottom: 0;
            background-color: #333; transition: .3s; border-radius: 20px;
        }
        .slider-switch:before {
            position: absolute; content: ""; height: 12px; width: 12px;
            left: 3px; bottom: 3px; background-color: #fff; transition: .3s; border-radius: 50%;
        }
        input:checked + .slider-switch { background-color: var(--main-red); }
        input:checked + .slider-switch:before { transform: translateX(16px); }

        #console-log {
            background: #000;
            border: 1px solid #1a1a1a;
            border-radius: 4px;
            height: 60px;
            margin-top: 15px;
            padding: 8px;
            font-family: monospace;
            font-size: 9px;
            color: #0f0;
            overflow-y: hidden;
            display: none;
        }

        .btn-inject {
            width: 100%;
            background: linear-gradient(45deg, #600, #f00);
            border: none; padding: 14px; color: white;
            font-weight: bold; border-radius: 6px; margin-top: 15px;
            cursor: pointer; text-transform: uppercase;
            box-shadow: 0 4px 15px rgba(255, 0, 0, 0.3);
            font-family: 'Orbitron', sans-serif;
        }

        #modal-ff {
            display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%;
            background: rgba(0, 0, 0, 0.95); z-index: 10000;
            justify-content: center; align-items: center; backdrop-filter: blur(15px);
        }
        .modal-content {
            background: #0d0d0f; padding: 25px; border-radius: 15px;
            border: 2px solid var(--main-red); text-align: center; width: 300px;
        }
        .ff-logo { width: 100%; border-radius: 10px; cursor: pointer; transition: 0.3s; }
        .ff-logo:hover { transform: scale(1.05); }

        #progress-container { width: 100%; background: #111; height: 8px; border-radius: 4px; margin-top: 15px; display: none; }
        #progress-bar { width: 0%; height: 100%; background: var(--main-red); box-shadow: 0 0 10px #f00; }
    </style>
</head>
<body>

<canvas id="particles"></canvas>
<div id="fov-circle"></div>

<div id="modal-ff">
    <div class="modal-content">
        <img src="https://wallpapers.com/images/featured/free-fire-jujutsu-kaisen-8mx0cbe84931t39z.jpg" class="ff-logo" id="btn-ff-action">
        <div style="color:var(--main-red); margin-top:15px; font-weight:bold; font-size:12px;">DOMAIN EXPANSION ACTIVE</div>
        <p style="font-size: 9px; color: #555;">SISTEMA INJETADO COM SUCESSO</p>
    </div>
</div>

<div class="container" id="main-panel">
    <h2>AUXÍLIO @PG <span style="font-size: 10px; vertical-align: middle; opacity: 0.5;">v2.0</span></h2>
    
    <div class="main-layout">
        <div class="sidebar">
            <div class="nav-item active" onclick="openTab('precision')">Precision</div>
            <div class="nav-item" onclick="openTab('fps')">FPS</div>
            <div class="nav-item" onclick="openTab('bypass')">By-Pass</div>
            <div class="nav-item" onclick="openTab('info')">Status</div>
        </div>

        <div class="content-area">
            <div id="precision" class="tab-content active">
                <div class="options-grid">
                    <div class="option-box"><span>Auto-Aim</span><label class="switch-container"><input type="checkbox" class="vibrate-sw prec-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>NoRecoil</span><label class="switch-container"><input type="checkbox" class="vibrate-sw prec-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>FOV Circle</span><label class="switch-container"><input type="checkbox" id="toggleFov" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>Anti-Shake</span><label class="switch-container"><input type="checkbox" class="vibrate-sw prec-sw"><span class="slider-switch"></span></label></div>
                    <div class="full-width">
                        <span class="label-style">Range: <span id="fovValue">0</span>m</span>
                        <input type="range" min="0" max="10" value="0" id="fovSlider">
                    </div>
                </div>
            </div>

            <div id="fps" class="tab-content">
                <div class="options-grid">
                    <div class="option-box"><span>Boost FPS</span><label class="switch-container"><input type="checkbox" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>Fast Render</span><label class="switch-container"><input type="checkbox" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                </div>
            </div>

            <div id="bypass" class="tab-content">
                <div class="options-grid">
                    <div class="option-box"><span>Invisível</span><label class="switch-container"><input type="checkbox" id="sw-bypass" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>Hide FOV</span><label class="switch-container"><input type="checkbox" id="sw-hide" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                    <div class="option-box"><span>Hard Reset</span><label class="switch-container"><input type="checkbox" id="sw-close-all" class="vibrate-sw"><span class="slider-switch"></span></label></div>
                </div>
            </div>

            <div id="info" class="tab-content">
                <div style="font-size: 10px; color: #888; border-left: 2px solid var(--main-red); padding-left: 10px;">
                    USER: PREMIUM_@PG<br>
                    EXPIRES: 31/12/2026<br>
                    SERVER: GLOBAL 01<br>
                    PROTECTION: ACTIVE
                </div>
            </div>
        </div>
    </div>

    <div id="console-log"></div>
    <div id="progress-container"><div id="progress-bar"></div></div>
    <button class="btn-inject">INICIAR INJEÇÃO</button>
</div>

<script>
    const fovCircle = document.getElementById('fov-circle');
    const toggleFov = document.getElementById('toggleFov');
    const fovSlider = document.getElementById('fovSlider');
    const btnInject = document.querySelector('.btn-inject');
    const modalFF = document.getElementById('modal-ff');
    const progressBar = document.getElementById('progress-bar');
    const progressContainer = document.getElementById('progress-container');
    const mainPanel = document.getElementById('main-panel');
    const consoleLog = document.getElementById('console-log');

    const swBypass = document.getElementById('sw-bypass');
    const swHide = document.getElementById('sw-hide');
    const swCloseAll = document.getElementById('sw-close-all');

    document.querySelectorAll('.vibrate-sw, .nav-item, .btn-inject').forEach(el => {
        el.addEventListener('click', () => { if(navigator.vibrate) navigator.vibrate(40); });
    });

    function updateFovCircle() {
        const val = parseInt(fovSlider.value);
        if (val === 0 || !toggleFov.checked || swHide.checked || swBypass.checked) { 
            fovCircle.style.display = 'none'; 
        } else { 
            fovCircle.style.display = 'block'; 
            fovCircle.style.width = (val * 45) + 'px'; 
            fovCircle.style.height = (val * 45) + 'px'; 
        }
    }

    swBypass.addEventListener('change', function() {
        mainPanel.style.opacity = this.checked ? "0.15" : "1";
        updateFovCircle();
    });

    swHide.addEventListener('change', updateFovCircle);

    swCloseAll.addEventListener('change', function() {
        if(this.checked) {
            document.querySelectorAll('.prec-sw').forEach(sw => sw.checked = false);
            toggleFov.checked = false; fovSlider.value = 0;
            document.getElementById('fovValue').innerHTML = "0";
            updateFovCircle();
            setTimeout(() => { this.checked = false; }, 500);
        }
    });

    fovSlider.oninput = function() { document.getElementById('fovValue').innerHTML = this.value; updateFovCircle(); }
    toggleFov.onchange = updateFovCircle;

    function openTab(tabName) {
        document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
        document.querySelectorAll('.nav-item').forEach(i => i.classList.remove('active'));
        document.getElementById(tabName).classList.add('active');
        event.currentTarget.classList.add('active');
    }

    btnInject.addEventListener('click', () => {
        btnInject.style.display = 'none';
        consoleLog.style.display = 'block';
        progressContainer.style.display = 'block';
        
        const logs = [
            "> Initializing @Pg bypass...",
            "> Searching com.dts.freefireth...",
            "> Memory offset redirected...",
            "> Patching security layer...",
            "> Injecting Precision.dll...",
            "> Domain Expansion applied!"
        ];

        let i = 0;
        const logInt = setInterval(() => {
            if(i < logs.length) {
                consoleLog.innerHTML += logs[i] + "<br>";
                i++;
            } else { clearInterval(logInt); }
        }, 600);

        let w = 0;
        const intr = setInterval(() => {
            if (w >= 100) { clearInterval(intr); modalFF.style.display = 'flex'; }
            else { w++; progressBar.style.width = w + '%'; }
        }, 40);
    });

    document.getElementById('btn-ff-action').addEventListener('click', () => {
        location.reload();
    });

    const canvas = document.getElementById('particles');
    const ctx = canvas.getContext('2d');
    canvas.width = window.innerWidth; canvas.height = window.innerHeight;
    let particles = [];
    class P {
        constructor() { this.x = Math.random()*canvas.width; this.y = Math.random()*canvas.height; this.sx = Math.random()*0.8-0.4; this.sy = Math.random()*0.8-0.4; }
        update() { this.x+=this.sx; this.y+=this.sy; if(this.x>canvas.width||this.x<0)this.sx*=-1; if(this.y>canvas.height||this.y<0)this.sy*=-1; }
        draw() { ctx.fillStyle = 'rgba(255, 0, 0, 0.5)'; ctx.beginPath(); ctx.arc(this.x,this.y,1.5,0,Math.PI*2); ctx.fill(); }
    }
    for(let i=0;i<40;i++) particles.push(new P());
    function anim() {
        ctx.clearRect(0,0,canvas.width,canvas.height);
        particles.forEach(p => { p.update(); p.draw(); });
        requestAnimationFrame(anim);
    }
    anim();
</script>
</body>
</html> 
